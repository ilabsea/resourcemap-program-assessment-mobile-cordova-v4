{{#if field_collections }}
  <div></div>
{{else}}
  <div class='empty-content'></div>
{{/if}}

<div data-role="collapsible-set" id='site-layers-wrapper'>
  {{#each field_collections}}
    <div  data-role="collapsible"
          data-inset="true"
          data-theme="a"
          data-mini="true"
          data-collapsed="true"
          data-id="{{id_wrapper}}"
          id="collapsable_update_online_{{id_wrapper}}">
      <h3>{{name_wrapper}}</h3>
    </div>
  {{/each}}
</div>
